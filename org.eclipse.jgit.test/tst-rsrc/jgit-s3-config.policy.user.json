{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BucketList",
            "Effect": "Allow",
            "Action": "s3:ListAllMyBuckets",
            "Resource": [
                "arn:aws:s3:::jgit.eclipse.org"
            ]
        },
        {
            "Sid": "BucketFullControl",
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": [
                "arn:aws:s3:::jgit.eclipse.org",
                "arn:aws:s3:::jgit.eclipse.org/*"
            ]
        }
    ]
}
